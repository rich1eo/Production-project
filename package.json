    "ts-loader": "^9.2.6",
    "ts-morph": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.4",
    "vite": "^5.2.8",
    "vite-plugin-svgr": "^4.2.0",
    "webpack": "^5.76.0",
